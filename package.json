{
  "name": "@ucp-npm/universal-connect-widget",
  "version": "0.1.0",
  "source": "src/index.tsx",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/cjs/types/index.d.ts",
  "config-overrides-path": "./config-overrides.cjs",
  "type": "module",
  "private": false,
  "author": "UCP",
  "license": "Apache-2.0",
  "description": "Whether you are an established financial institution with existing data providers or a one person startup the Universal Connect Project (UCP) is the right way to connect to financial services like banks, credit unions and wealth managers.",
  "bugs": {
    "url": "https://github.com/Universal-Connect-Project/universal-connect-widget/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Universal-Connect-Project/universal-connect-widget.git"
  },
  "keywords": [
    "Universal",
    "Connect",
    "Widget",
    "Fintech",
    "Finance",
    "Consumer"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "package": "npm run clean && npm run package:esm",
    "package:esm": "tsc --jsx react-jsx -p ./tsconfig.esm.json && tsc-esm-fix --tsconfig ./tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs && npm run copy-files-esm",
    "package:cjs": "tsc --jsx react-jsx -p ./tsconfig.cjs.json && mv dist/cjs/index.js dist/cjs/index.cjs && npm run copy-files-cjs",
    "copy-files-esm": "copyfiles -u 1 \"./src/**/*.svg\" \"./src/**/*.css\" dist/esm",
    "copy-files-cjs": "copyfiles -u 1 \"./src/**/*.svg\" \"./src/**/*.css\" dist/cjs",
    "build": "react-app-rewired build",
    "build-android": "npx cross-env mobile=true react-app-rewired build && npx cap sync android",
    "build-ios": "npx cross-env mobile=true react-app-rewired build && npx cap sync ios",
    "prepack": "npm run build",
    "dev": "npx cross-env BROWSER=none react-app-rewired start",
    "test-watch": "react-app-rewired test --watchAll",
    "test": "jest --no-coverage",
    "coverage": "npm test -- --verbose --coverage --watchAll",
    "test-clear": "npm test -- --clearCache",
    "keys": "node -e 'console.log(`key: ${crypto.randomBytes(32).toString(\"hex\")}\n IV: ${crypto.randomBytes(16).toString(\"hex\")}`)'",
    "lint": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "lint-check": "eslint --cache --ext .js,.jsx,.ts,.tsx",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.14.6",
    "@capacitor-community/http": "^1.4.1",
    "@capacitor/android": "4.7.3",
    "@capacitor/core": "^4.7.3",
    "@capacitor/ios": "4.7.3",
    "@kyper/button": "^3.2.0",
    "@kyper/card": "^2.0.0",
    "@kyper/icon": "^1.15.0",
    "@kyper/input": "^5.11.0",
    "@kyper/institutionlogo": "^2.2.0",
    "@kyper/messagebox": "^2.2.1",
    "@kyper/progressindicators": "^2.1.1",
    "@kyper/select": "^1.4.0",
    "@kyper/selectionbox": "^1.2.0",
    "@kyper/text": "^2.4.2",
    "@kyper/textarea": "^3.3.0",
    "@kyper/tokenprovider": "^3.2.0",
    "@kyper/userfeedback": "^1.2.0",
    "@kyper/utilityrow": "^1.4.0",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-intl": "^3.0.0",
    "@types/redis": "^4.0.11",
    "assert-browserify": "^2.0.0",
    "axios": "^1.6.0",
    "bowser": "^2.11.0",
    "buffer-browserify": "^0.2.5",
    "core-js": "^3.29.1",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "d3": "^7.8.5",
    "date-fns": "^2.29.3",
    "dompurify": "^2.3.6",
    "dotenv": "^16.3.1",
    "downshift": "^7.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.0.2",
    "gettext.js": "^1.2.0",
    "honeybadger-js": "^0.5.1",
    "intl": "^1.2.5",
    "js-logger": "^1.6.1",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mx-design-tokens": "^10.5.0",
    "mx-react-components": "^8.7.0",
    "phoenix": "^1.2.1",
    "posthog-js": "^1.57.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^2.3.0",
    "react-redux": "^7.1.0",
    "redis": "^4.6.12",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-observable": "^1.0.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rxjs": "^6.6.2",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.2.2",
    "url": "^0.11.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.18.6",
    "@capacitor/cli": "^4.8.1",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "babel-plugin-js-logger": "^1.0.17",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "cypress": "^13.6.2",
    "eslint-config-mx": "^4.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-namespaced-actions": "^1.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "terser-webpack-plugin": "^5.3.7",
    "ts-import-plugin": "^3.0.0",
    "ts-jest": "^29.1.2",
    "tsc-esm-fix": "^2.20.24",
    "url-loader": "^4.1.1"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint-check"
    ]
  }
}
